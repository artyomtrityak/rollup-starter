{
  "name": "rollup-starter-template",
  "version": "1.0.0",
  "description": "Template for Babel (preset-env), Mocha tests, prettier, eslint, JavaScript rollup bundler.",
  "main": "dist/bundle.js",
  "scripts": {
    "watch": "cross-env NODE_ENV=production rollup -c -w",
    "build": "cross-env NODE_ENV=production rollup -c",
    "test": "cross-env NODE_ENV=test mocha tests/* --recursive --compilers js:babel-register",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artyomtrityak/rollup-starter.git"
  },
  "keywords": [
    "rollup",
    "template",
    "JavaScript"
  ],
  "author": "Artem Trytiak <art.trityak@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artyomtrityak/rollup-starter/issues"
  },
  "homepage": "https://github.com/artyomtrityak/rollup-starter#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-config-fbjs": "^2.0.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.48.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-relay": "0.0.21",
    "lint-staged": "^7.1.2",
    "mocha": "^5.2.0",
    "prettier": "^1.13.3",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0"
  }
}
